{
  "name": "mosquitodb",
  "version": "1.0.0",
  "description": "MosquitoDB is a NoSQL database engine adapted with super fast, memory efficient, secured and fully queried features",
  "main": "lib/index.js",
  "type": "module",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "mongo-stop": "brew services stop mongodb-community",
    "mongo-run": "brew services run mongodb-community",
    "mongo-list": "brew services list",
    "build": "rm -rf dist && NODE_ENV=production babel lib --out-dir dist --copy-files",
    "start": "nodemon --exec babel-node --experimental-modules --es-module-specifier-resolution=node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deflexable/mosquitodb.git"
  },
  "keywords": [
    "javascript",
    "mosquitodb",
    "selfhosted",
    "self-database",
    "database",
    "serverless",
    "BaaS",
    "firebase",
    "fully queried"
  ],
  "author": "Anthony Onabanjo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deflexable/mosquitodb/issues"
  },
  "homepage": "https://github.com/deflexable/mosquitodb#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "google-auth-library": "^8.8.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "mongodb": "^5.3.0",
    "node-fetch": "^3.3.1",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.5",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "eslint": "^8.23.1"
  },
  "engines": {
    "node": ">=14"
  }
}
